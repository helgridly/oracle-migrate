{
  "name": "oracle-migrate",
  "version": "0.0.1",
  "description": "Migration framework for node and Oracle DB",
  "main": "index.js",
  "bin": {
    "oracle-migrate": "./bin/migrate"
  },
  "scripts": {
    "start": "node ./bin/migrate",
    "create": "node ./bin/migrate create",
    "up": "node ./bin/migrate up",
    "down": "node ./bin/migrate down",
    "downall": "node ./bin/migrate down all",
    "history": "node ./bin/migrate history",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/olehch/oracle-migrate.git"
  },
  "keywords": [
    "database",
    "oracle",
    "migrate",
    "migrations"
  ],
  "engines": {
    "node": ">= 0.6.x"
  },
  "homepage": "git://github.com/olehch/oracle-migrate",
  "author": "Oleh Chaplya",
  "license": "MIT",
  "dependencies": {
    "dateformat": "^1.0.12",
    "oracledb": "^1.11.0",
    "simple-oracledb": "^1.1.21"
  }
}
